{
    "version": 1,
    "builds": [
      {
        "src": "entry.js",
        "use": "@vercel/node"
      },
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "^/$",
        "dest": "/entry.js"
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
}